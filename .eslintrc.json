{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "semi": 0,
        "react/jsx-props-no-spreading": 0,
        "react/forbid-prop-types": 0,
        "no-lonely-if": 0,
        "class-methods-use-this": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "camelcase": 0
    }
}
